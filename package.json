{
  "name": "ipfs-interop",
  "version": "8.0.0",
  "description": "Interoperability Tests for IPFS",
  "author": "David Dias <daviddias@ipfs.io>",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/interop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/interop.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/interop/issues"
  },
  "keywords": [
    "IPFS"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "bin": {
    "ipfs-interop": "bin/ipfs-interop.js"
  },
  "main": "src/index.js",
  "type": "module",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    },
    "ignorePatterns": [
      "package.json"
    ]
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Trivial Changes"
              },
              {
                "type": "docs",
                "section": "Trivial Changes"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "clean": "rimraf /tmp/js-ipfs /tmp/go-ipfs ./go-libp2p-relay-daemon dist",
    "lint": "aegir lint",
    "build": "aegir build",
    "release": "aegir release",
    "postinstall": "cross-env node ./scripts/setup-libp2p-relay-daemon.js",
    "pretest": "aegir build",
    "test": "cross-env LIBP2P_TCP_REUSEPORT=false aegir test",
    "test:node": "cross-env LIBP2P_TCP_REUSEPORT=false aegir test -t node -f test/node.js",
    "test:browser": "cross-env LIBP2P_TCP_REUSEPORT=false aegir test -t browser -f test/browser.js",
    "dep-check": "aegir dep-check package.json"
  },
  "dependencies": {
    "@libp2p/peer-id": "^1.1.10",
    "@libp2p/webrtc-star-signalling-server": "^1.0.4",
    "@libp2p/websockets": "^1.0.8",
    "@types/is-ci": "^3.0.0",
    "aegir": "^37.0.15",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "cachedir": "^2.3.0",
    "cross-env": "^7.0.2",
    "delay": "^5.0.0",
    "execa": "^6.1.0",
    "go-platform": "^1.0.0",
    "got": "^12.0.0",
    "gunzip-maybe": "^1.4.2",
    "hasha": "^5.2.2",
    "import-meta-resolve": "^1.1.1",
    "ipfs-unixfs": "^6.0.3",
    "ipfs-utils": "^9.0.1",
    "ipfsd-ctl": "^11.0.0",
    "ipns": "^1.0.0",
    "is-ci": "^3.0.0",
    "iso-random-stream": "^2.0.0",
    "it-all": "^1.0.1",
    "it-concat": "^2.0.0",
    "it-drain": "^1.0.0",
    "it-last": "^1.0.1",
    "it-to-buffer": "^2.0.1",
    "multiformats": "^9.3.0",
    "nanoid": "^3.1.10",
    "p-defer": "^4.0.0",
    "p-retry": "^5.1.1",
    "pretty-bytes": "^6.0.0",
    "promisify-es6": "^1.0.3",
    "random-fs": "^1.0.3",
    "rimraf": "^3.0.2",
    "tar-fs": "^2.1.1",
    "uint8arrays": "^3.0.0",
    "unzip-stream": "^0.3.1",
    "wherearewe": "^1.0.0"
  },
  "devDependencies": {
    "go-ipfs": "^0.12.2",
    "ipfs": "^0.62.3",
    "ipfs-http-client": "^56.0.3"
  },
  "browser": {
    "go-ipfs": false,
    "fs": false,
    "execa": false
  }
}
